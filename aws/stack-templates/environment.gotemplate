{{{define "environment"}}}
{
  "Name": "AWS_REGION",
  "Value": {
    "Ref": "AWS::Region"
  }
},
{
  "Name": "AWS_DEFAULT_REGION",
  "Value": {
    "Ref": "AWS::Region"
  }
},
{
  "Name": "LANG",
  "Value": "en_US.UTF-8"
},
{
  "Name": "LC_ALL",
  "Value": "en_US.UTF-8"
},
{
  "Name": "POSTGRES_USER",
  "Value": {
    "Ref": "PostgresUser"
  }
},
{
  "Name": "POSTGRES_PASSWORD",
  "Value": {
    "Ref": "PostgresPassword"
  }
},
{
  "Name": "SL_MIGRATIONS_PGHOST",
  "Value": {
    "Fn::GetAtt": [
      "MigrationsPostgres",
      "Endpoint.Address"
    ]
  }
},
{
  "Name": "SL_MIGRATIONS_PGPASSWORD",
  "Value": {
    "Ref": "SlMigrationsPgPassword"
  }
},
{
  "Name": "PGSSLMODE",
  "Value": "require"
},
{
  "Name": "PGTZ",
  "Value": "UTC"
},
{
  "Name": "TZ",
  "Value": "UTC"
},
{
  "Name": "SL_STACK_NAME",
  "Value": {
    "Ref": "AWS::StackName"
  }
},
{
  "Name": "SL_GIT_SHA",
  "Value": {
    "Ref": "SlGitSha"
  }
},
{
  "Name": "SL_LOCAL",
  "Value": "false"
},
{
  "Name": "SL_LOAD_BALANCER_HOSTNAME",
  "Value": {
    "Ref": "SlLoadBalancerHostname"
  }
},
{
  "Name": "SL_LOAD_BALANCER_SCHEME",
  "Value": "https"
},
{
  "Name": "SL_LOG_COLORIZATION",
  "Value": "false"
},
{
  "Name": "SL_LOG_DEBUG_MESSAGES",
  "Value": "false"
},
{
  "Name": "SL_LOG_SERIALIZATION",
  "Value": "json-compact"
}
{{{end}}}
